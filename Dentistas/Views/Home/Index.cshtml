                                </a>
                                <h4 class="destination-place">
                                    <a href="@Url.Action("Details", "Odontologos", new { id = item.ID})">@item.Especialidad</a>
                                </h4>
                                <a href="#" class="list-like left"><i class="ti-heart"></i></a>
                            </figure>
                        </div>

